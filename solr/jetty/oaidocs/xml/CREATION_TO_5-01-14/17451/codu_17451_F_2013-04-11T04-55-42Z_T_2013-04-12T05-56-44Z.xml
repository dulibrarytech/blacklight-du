  <dc:title>Bed Dedication Ceremony at the Jewish Consumptives&apos; Relief Society</dc:title>
  <dc:relation>Beck Archives Photograph Collection, Health Care and Philanthropy</dc:relation>
  <dc:date>between 1904-1928</dc:date>
  <dc:publisher>University of Denver</dc:publisher>
  <dc:publisher>Denver, Colorado</dc:publisher>
  <dc:type>StillImage</dc:type>
